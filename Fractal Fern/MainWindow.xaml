        <Label Content="Size" Height="44" Margin="730,104,-160,0" Name="label1" VerticalAlignment="Top" FontSize="20" FontWeight="Bold" FontFamily="MV Boli" />
        <Label Content="Redux" Height="50" HorizontalAlignment="Left" Margin="720,168,0,0" Name="label2" VerticalAlignment="Top" FontSize="20" Width="110" FontWeight="Bold" FontFamily="MV Boli" />
        <Label Content="Turn Bias" Height="51" HorizontalAlignment="Left" Margin="706,236,0,0" Name="label3" VerticalAlignment="Top" FontFamily="MV Boli" FontSize="20" Width="126" FontWeight="Bold" />
    </Grid>
</Window>