        <Slider Height="23" HorizontalAlignment="Left" Margin="665,208,0,0" Name="reduxSlider" VerticalAlignment="Top" Width="175" Value="3" Maximum="5" Minimum="1.5" />
        <Slider Height="23" HorizontalAlignment="Left" Margin="665,271,0,0" Name="biasSlider" VerticalAlignment="Top" Width="175" Value="0.3" Maximum="1.0" Minimum="0.0" ValueChanged="BiasSlider_ValueChanged" />
